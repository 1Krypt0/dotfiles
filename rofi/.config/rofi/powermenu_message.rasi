/* Message Dialog */

@import "colors.rasi"

* {
    background-color:       @bg-col;
    text-color:             @fg-col2;
	font:				 	"Iosevka Nerd Font 12";
}

window {
    width:      			360px;
    padding:    			25px;
    border:					1px;
    border-radius:          0px;
    border-color:           @bg-col;
    location:               center;
    y-offset:               -2em;
}

entry {
    expand: 				true;
    text-color:				@fg-col2;
}
